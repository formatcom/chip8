{
  "name": "chip-8",
  "version": "0.0.1",
  "description": "emulator chip-8",
  "main": "index.coffee",
  "scripts": {
    "build": "mkdir -p dist && browserify --ignore canvas --transform coffeeify --extension=coffee chip8.coffee > dist/chip8.js && jade index.jade --pretty --out dist",
    "server": "cd dist && python -m SimpleHTTPServer 8080",
    "clean": "rm -rf dist *.log",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/formatcom/chip8.git"
  },
  "keywords": [
    "emulator",
    "chip8",
    "chip-8",
    "coffee",
    "coffeescript",
    "javascript",
    "html5"
  ],
  "author": "Vinicio Valbuena (FormatCom)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/formatcom/chip8/issues"
  },
  "homepage": "https://github.com/formatcom/chip8#readme",
  "devDependencies": {
    "browserify": "^12.0.1",
    "coffee-script": "^1.10.0",
    "coffeeify": "^2.0.1",
    "jade": "^1.11.0"
  },
  "dependencies": {
    "canvas": "^1.3.6",
    "is-node": "0.0.0",
    "minimist": "^1.2.0"
  }
}
